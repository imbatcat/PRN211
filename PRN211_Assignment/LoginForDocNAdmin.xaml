        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRN211_Assignment"
        mc:Ignorable="d"
        Title="LoginForDocNAdmin" Height="450" Width="800">
    <Grid>
        <Label Content="Login for Doctor and Admin" HorizontalAlignment="Left" Margin="278,49,0,0" VerticalAlignment="Top" Height="33" Width="224" FontSize="16" FontWeight="Bold" FontStyle="Italic"/>
