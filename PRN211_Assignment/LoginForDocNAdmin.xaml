        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="424,256,0,0" VerticalAlignment="Top" Height="77" Width="176" FontWeight="Bold" FontSize="18" Click="btnLogin_Click"/>
        <Button x:Name="btnBackPage" Content="Back" HorizontalAlignment="Left" Margin="193,256,0,0" VerticalAlignment="Top" Height="77" Width="176" FontWeight="Bold" FontSize="18" Click="btnBackPage_Click"/>

    </Grid>
