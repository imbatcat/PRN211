        <Label Content="Password" HorizontalAlignment="Left" Margin="257,162,0,0" VerticalAlignment="Top" Width="90"/>
        <TextBox Name="txtUser" HorizontalAlignment="Left" Margin="347,105,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="130" Height="35"/>
